<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity"
    android:background="#16191E">

    <TextView
        android:id="@+id/some_id"
        android:layout_width="300dp"
        android:layout_height="50dp"
        android:layout_marginTop="35dp"
        android:layout_marginStart="40dp"
        android:fontFamily="@font/intermedium"
        android:text="@string/some_id"
        android:textAppearance="@style/some_id"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintBottom_toTopOf="@id/some_id2"/>

    <TextView
        android:id="@+id/some_id2"
        android:layout_width="124dp"
        android:layout_height="44dp"
        android:fontFamily="@font/interbold"
        android:gravity="top"
        android:text="@string/some_id2"
        android:textAppearance="@style/some_id2"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginStart="40dp"
        app:layout_constraintTop_toBottomOf="@id/some_id"
        android:layout_marginTop="40dp"
        app:layout_constraintBottom_toTopOf="@id/ellipse_1"/>

    <Button
        android:onClick="OnClickNumbers"
        android:id="@+id/ellipse_1"
        android:layout_width="88dp"
        android:layout_height="88dp"
        android:layout_marginStart="14dp"
        android:layout_marginTop="50dp"
        android:background="@drawable/circle"
        android:text="7"
        android:textSize="40dp"
        app:backgroundTint="@null"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/some_id2"
        app:layout_constraintEnd_toStartOf="@id/ellipse_2"/>
    <Button
        android:onClick="OnClickNumbers"
        android:id="@+id/ellipse_2"
        android:layout_width="88dp"
        android:layout_height="88dp"
        android:layout_marginStart="14dp"
        android:layout_marginTop="50dp"
        android:background="@drawable/circle"
        app:backgroundTint="@null"
        android:text="8"
        android:textSize="40dp"
        app:layout_constraintStart_toEndOf="@id/ellipse_1"
        app:layout_constraintTop_toBottomOf="@id/some_id2"
        app:layout_constraintEnd_toStartOf="@id/ellipse_3"/>

    <Button
        android:onClick="OnClickNumbers"
        android:id="@+id/ellipse_3"
        android:layout_width="88dp"
        android:layout_height="88dp"
        android:layout_marginStart="17dp"
        android:layout_marginTop="50dp"
        android:background="@drawable/circle"
        app:backgroundTint="@null"
        android:text="9"
        android:textSize="40dp"
        app:layout_constraintStart_toEndOf="@id/ellipse_2"
        app:layout_constraintTop_toBottomOf="@id/some_id2"
        app:layout_constraintEnd_toStartOf="@id/ellipse_4"/>

    <Button
        android:onClick="OnClickNumbers"
        android:id="@+id/ellipse_4"
        android:layout_width="88dp"
        android:layout_height="88dp"
        android:layout_marginStart="17dp"
        android:layout_marginTop="50dp"
        android:layout_marginEnd="14dp"
        android:background="@drawable/cir"
        app:backgroundTint="@null"
        android:text="/"
        android:textSize="40dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@id/ellipse_3"
        app:layout_constraintTop_toBottomOf="@id/some_id2" />

    <Button
        android:onClick="OnClickNumbers"
        android:id="@+id/ellipse_5"
        android:layout_width="88dp"
        android:layout_height="88dp"
        android:layout_marginStart="14dp"
        android:layout_marginTop="17dp"
        android:background="@drawable/circle"
        app:backgroundTint="@null"
        android:text="4"
        android:textSize="40dp"
        app:layout_constraintTop_toBottomOf="@id/ellipse_1"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toStartOf="@id/ellipse_6"
        />

    <Button
        android:onClick="OnClickNumbers"
        android:id="@+id/ellipse_6"
        android:layout_width="88dp"
        android:layout_height="88dp"
        android:layout_marginStart="14dp"
        android:layout_marginTop="17dp"
        android:background="@drawable/circle"
        app:backgroundTint="@null"
        android:text="5"
        android:textSize="40dp"
        app:layout_constraintTop_toBottomOf="@id/ellipse_2"
        app:layout_constraintStart_toEndOf="@id/ellipse_5"
        app:layout_constraintEnd_toStartOf="@id/ellipse_7"
        />

    <Button
        android:onClick="OnClickNumbers"
        android:id="@+id/ellipse_7"
        android:layout_width="88dp"
        android:layout_height="88dp"
        android:layout_marginStart="14dp"
        android:layout_marginTop="17dp"
        android:background="@drawable/circle"
        app:backgroundTint="@null"
        android:text="6"
        android:textSize="40dp"
        app:layout_constraintTop_toBottomOf="@id/ellipse_3"
        app:layout_constraintStart_toEndOf="@id/ellipse_6"
        app:layout_constraintEnd_toStartOf="@id/ellipse_8"
        />

    <Button
        android:onClick="OnClickNumbers"
        android:id="@+id/ellipse_8"
        android:layout_width="88dp"
        android:layout_height="88dp"
        android:layout_marginStart="17dp"
        android:layout_marginTop="17dp"
        android:layout_marginEnd="16dp"
        android:background="@drawable/cir"
        app:backgroundTint="@null"
        android:text="*"
        android:textSize="40dp"
        app:layout_constraintTop_toBottomOf="@id/ellipse_4"
        app:layout_constraintStart_toEndOf="@id/ellipse_7"
        app:layout_constraintEnd_toEndOf="parent"
        />

    <Button
        android:onClick="OnClickNumbers"
        android:id="@+id/ellipse_9"
        android:layout_width="88dp"
        android:layout_height="88dp"
        android:layout_marginStart="7.6dp"
        android:layout_marginTop="17dp"
        android:background="@drawable/circle"
        app:backgroundTint="@null"
        android:text="1"
        android:textSize="40dp"
        app:layout_constraintEnd_toStartOf="@id/ellipse_10"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/ellipse_5" />

    <Button
        android:id="@+id/ellipse_10"
        android:layout_width="88dp"
        android:layout_height="88dp"
        android:layout_marginStart="15dp"
        android:layout_marginTop="16dp"
        android:background="@drawable/circle"
        android:onClick="OnClickNumbers"
        android:text="2"
        android:textSize="40dp"
        app:backgroundTint="@null"
        app:layout_constraintEnd_toStartOf="@id/ellipse_3"
        app:layout_constraintStart_toEndOf="@id/ellipse_9"
        app:layout_constraintTop_toBottomOf="@id/ellipse_6" />

    <Button
        android:onClick="OnClickNumbers"
        android:id="@+id/ellipse_11"
        android:layout_width="88dp"
        android:layout_height="88dp"
        android:layout_marginStart="17dp"
        android:layout_marginTop="17dp"
        android:background="@drawable/circle"
        app:backgroundTint="@null"
        android:text="3"
        android:textSize="40dp"
        app:layout_constraintTop_toBottomOf="@id/ellipse_7"
        app:layout_constraintStart_toEndOf="@id/ellipse_10"
        app:layout_constraintEnd_toStartOf="@id/ellipse_12"
        />

    <Button
        android:onClick="OnClickNumbers"
        android:id="@+id/ellipse_12"
        android:layout_width="88dp"
        android:layout_height="88dp"
        android:layout_marginStart="17dp"
        android:layout_marginTop="17dp"
        android:layout_marginEnd="17dp"
        android:background="@drawable/cir"
        app:backgroundTint="@null"
        android:text="-"
        android:textSize="40dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@id/ellipse_11"
        app:layout_constraintTop_toBottomOf="@id/ellipse_8" />

    <Button
        android:onClick="OnClickNumbers"
        android:id="@+id/rec_1"
        android:layout_width="160dp"
        android:layout_height="70dp"
        android:background="@drawable/rectangle"
        app:backgroundTint="@null"
        android:text="0"
        android:textSize="40dp"
        android:layout_marginStart="17dp"
        android:layout_marginTop="25dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/ellipse_9"
        app:layout_constraintEnd_toStartOf="@id/ellipse_13"/>

    <Button
        android:onClick="OnClickNumbers"
        android:id="@+id/ellipse_13"
        android:layout_width="88dp"
        android:layout_height="88dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="16dp"
        android:background="@drawable/cir"
        android:text="."
        android:textSize="50dp"
        app:backgroundTint="@null"
        app:layout_constraintEnd_toStartOf="@id/ellipse_14"
        app:layout_constraintStart_toEndOf="@id/rec_1"
        app:layout_constraintTop_toBottomOf="@id/ellipse_11" />

    <Button
        android:onClick="OnClickNumbers"
        android:id="@+id/ellipse_14"
        android:layout_width="88dp"
        android:layout_height="88dp"
        android:layout_marginStart="17dp"
        android:layout_marginTop="17dp"
        android:layout_marginEnd="10dp"
        android:background="@drawable/cir"
        app:backgroundTint="@null"
        android:text="+"
        android:textSize="40dp"
        app:layout_constraintStart_toEndOf="@id/ellipse_13"
        app:layout_constraintTop_toBottomOf="@id/ellipse_12"
        app:layout_constraintEnd_toEndOf="parent"/>

    <Button
        android:onClick="OnClickNumbers"
        android:id="@+id/rec_2"
        android:layout_width="270dp"
        android:layout_height="75dp"
        android:background="@drawable/rectangle_r"
        app:backgroundTint="@null"
        android:layout_marginStart="30dp"
        android:layout_marginTop="30dp"
        android:text="="
        android:textSize="40dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/rec_1"
        app:layout_constraintEnd_toStartOf="@id/ellipse_15"/>

    <Button
        android:onClick="OnClickNumbers"
        android:id="@+id/ellipse_15"
        android:layout_width="88dp"
        android:layout_height="88dp"
        android:layout_marginStart="17dp"
        android:layout_marginTop="17dp"
        android:layout_marginEnd="30dp"
        android:background="@drawable/cir"
        app:backgroundTint="@null"
        android:text="C"
        android:textSize="40dp"
        app:layout_constraintStart_toEndOf="@id/rec_2"
        app:layout_constraintTop_toBottomOf="@id/ellipse_14"
        app:layout_constraintEnd_toEndOf="parent"/>

</androidx.constraintlayout.widget.ConstraintLayout>